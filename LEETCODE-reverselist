 def reverseList(self, head):
            if not head :
                return head
            if not head.next:
                return head
            tmp=head
            tmp2=tmp.next
            tmp3=None
            while(tmp2):     
                if tmp==head:
                    tmp.next=None                                                   
                tmp3=tmp2.next
                tmp2.next=tmp                 
                tmp=tmp2                
                tmp2=tmp3

            return tmp        
